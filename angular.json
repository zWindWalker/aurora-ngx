{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "prefix": "aurora-ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/blibs/uild-ng-packagr:build",
          "options": {
            "tsConfig": "libs/ui/tsconfig.lib.json",
            "project": "libs/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "libs/ui/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/blibs/uild-angular:karma",
          "options": {
            "main": "libs/ui/src/test.ts",
            "tsConfig": "libs/ui/tsconfig.spec.json",
            "karmaConfig": "libs/ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/blibs/uild-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "schematics": {
      "root": "libs/schematics",
      "sourceRoot": "libs/schematics/src",
      "projectType": "library",
      "prefix": "aurora-ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/schematics/tsconfig.lib.json",
            "project": "libs/schematics/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "libs/schematics/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/schematics/src/test.ts",
            "tsConfig": "libs/schematics/tsconfig.spec.json",
            "karmaConfig": "libs/schematics/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/schematics/tsconfig.lib.json",
              "libs/schematics/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  }
}
